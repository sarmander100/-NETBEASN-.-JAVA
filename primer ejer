           System.out.print ("NOCHE");
           
           int  a= 10;
           int  b= 30;
           int  n1, n2, n3, coco = 18;
           System.out.print(coco);
           System.out.println("LA SUMA DE"+a+"mas"+coco+ "y"+b+"es;");
           n1 =a+b+coco;
           System.out.println(n1);
           
           
   // PARA SACAR LA SUMA DE DOS NUMERO Y DE RESLTADO //
   
   
          System.out.println(" VAMOS A REALIZAR UNA SUMA ");
          Scanner tecla= new Scanner( System.in);
          System.out.println("INGRESE EL PRIMER NUMERO");
           n1 = tecla.nextInt();
          System.out.println("\n INGRESE EL SEGUNDO NUMERO");
